module "s3_bucket" {
  source = "terraform-aws-modules/s3-bucket/aws"

  bucket = "libera-bucket"
  acl    = "private"  # public-read -> Owner gets FULL_CONTROL. The AllUsers group gets READ access.

  control_object_ownership = true
  object_ownership         = "ObjectWriter"

  versioning = {
    enabled = true
  }
}
